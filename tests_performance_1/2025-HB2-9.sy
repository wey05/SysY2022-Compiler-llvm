// test correctness of loop optimize
// be careful with input data since it may causes undefined behavior

int max(int a,int b){
    if(a<b){
        return b;
    }else{
        return a;
    }
}

int f(int x){
    int t1 = max(x,2147483647-x);
    int t2 = max(t1,1073741823-t1);
    int t3 = max(t2,536870912-t2);
    return (t3+t3*3/1000*1001)%19491001;
}

const int mod = 998244853;

int loop_test(int s,int t,int d){
    int sum = 0;
    int x = s;
while (x < t) { 
        sum = (sum + f(x) + 1) % mod;
        x =x+ d; 
    }
    return sum;
}

int main(){
    int T = getint();
    starttime();
    while(T){
        int s = getint();
        int t = getint();
        int d = getint();
        putint(loop_test(s,t,d));
        putch(10);
        T = T-1;
    }
    stoptime();
    return 0;
}
